{
    "name": "tiat-terraform-autocomplete",
    "displayName": "Terraform Autocomplete for Tencent Cloud",
    "description": "Autocomplete for Tencnet Cloud resources with terraform.",
    "version": "0.0.3",
    "license": "MIT",
    "publisher": "nickyinluo",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "capabilities": {
        "completionProvider": {
            "resolveProvider": "true",
            "triggerCharacters": [
                "."
            ]
        }
    },
    "activationEvents": [
        "onLanguage:terraform"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "terraform",
                "aliases": [
                    "Terraform",
                    "terraform"
                ],
                "extensions": [
                    ".tf",
                    ".tfvars",
                    ".hcl"
                ],
                "configuration": "./terraform.configuration.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "@types/opn": "^3.0.28",
        "lodash": "^4.17.4",
        "opn": "^5.1.0"
    }
}